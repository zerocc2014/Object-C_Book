"-highlight"
{
  "plugins": ["-highlight","video","ga","codeblock-filename","emphasize","baidu","link-fixer","anchors","redirect","jsfiddle","ace"],
  "pluginsConfig": {  
    "ga": {
      "token": "UA-XXXX-Y",
      "configuration": {
         "cookieName": "new_cookie_name",
         "cookieDomain": "mynew.domain.com"
         }
      },
      
     "baidu": {
       "token": "YOUR TOKEN"
      },
      
     "link-fixer": {
        "link": "https://www.baseurl.com/"
      },
      
     "jsfiddle":{
            "type":"script",
            "tabs":["result","js","css", "html"],
            "height": "500",
            "width": "500",
            "fontColor": "00FF00"
          },
      "ace":{
             "languages": [
                {
                    "edit":"true",
                    "lang": "mode-objectivec.js",
                    "check":"true",
                    "theme":"theme-xcode.js"
                }
            ]
      
      }
          
          
  
 }
}